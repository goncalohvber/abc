#include <stdlib.h>
#include <stdio.h>
#include "Instalacao.h"
#include "Tarifas.h"
#include "Estacionamento.h"
#include <locale.h>
#include "Util.h"

int lerconfig(Confparque *config);
int gravarconfig(const Confparque *config);
int modificatarifa(Tarifa tarifas[], int *numtarifas);
int lertarifas(Tarifa tarifas[], int *numtarifas);
void mostrarTabela(Tarifa tarifas[], int n);
int guardartarifas(Tarifa tarifas[], int numtarifas);
int personalizapp(Confparque config);
void mostrarMenu(void);

int main(void) {
    
    setlocale(LC_ALL, "Portuguese");
    
    FILE *F1=NULL;
    F1=fopen("configfeita.txt", "r");
    Confparque config;
    if(!F1) {
        personalizapp(config);
    }
    else{
        mostrarMenu();
        
    }
    
    
}
